package br.com.stimuli.loading
{
	import br.com.stimuli.loading.loadingtypes.LoadingItem;
	
	import flash.events.Event;

	public class BulkItemEvent extends Event
	{
		public var item:LoadingItem;
		public function BulkItemEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false,_item:LoadingItem=null)
		{
			super(type, bubbles, cancelable);
			this.item=_item;
		}
		
	}
}